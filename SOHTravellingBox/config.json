{
  "id": "germany_autobahn_truck_simulation",
  "globals": {
    "deltaT": 1,
    "startPoint": "2024-11-11T17:00:00",
    "endPoint": "2024-11-11T17:15:00",
    "deltaTUnit": "seconds",
    "console": true,
    "npgSqlOptions": {
      "host": "localhost",
      "user": "mars",
      "password": "sram2020",
      "overrideByConflict": true
    }
  },
  "agents": [
    {
      "name": "SemiTruckDriver",
      "outputs": [
        {
          "type": "geojson",
          "kind": "trips"
        }
      ],
      "individual": [
        {
          "value": true,
          "parameter": "ResultTrajectoryEnabled"
        }
      ],
      "outputFilter": [
        
      ]
    }
  ],
  "layers": [
    {
      "name": "SemiTruckLayer",
      "inputs": [
        {
          "file": "resources/autobahn_and_bundesstreet.geojson",
          "inputConfiguration": {
            "modalities": [
              "CarDriving"
            ],
            "isBidirectedGraph": true
          }
        }
      ],
      "outputFrequency": 60,
      "output": "geojson"
    },
    {
      "name": "SemiTruckSchedulerLayer",
      "file": "resources/semi_truck_scheduler.csv"
    }
  ],
  "entities": [
    {
      "name": "SemiTruck",
      "file": "resources/semi_truck.csv"
    }
  ]
}
